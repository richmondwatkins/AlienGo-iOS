# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

def sharedPods
    pod 'Alamofire', '4.0.1'
    pod 'PINRemoteImage'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'KeychainAccess'
end

target 'Alien Reader' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  sharedPods

end

target 'NewsReader' do
    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!

    sharedPods
end
